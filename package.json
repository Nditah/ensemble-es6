  "license": "MIT",
  "homepage": "https://github.com/nditah/backend-es6#readme",
  "main": "dist",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec babel-node ./src --trace-warnings",
    "serve": "NODE_ENV=production node ./dist  --source-maps",
    "apprunner": "pm2 start ./dist -i 1 --name erp --env production --update-env",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --require @babel/register --recursive  --timeout 10000",
    "kill": "sudo kill $(sudo lsof -t -i:5010)",
    "doc": "rm -rf dist/public && apidoc -i src/ -o dist/public",
    "dist": "babel src -s -D -d dist && npm run doc",
    "rep": "run-rs --mongod --keep --shell replStart.js --dbpath /home/user/Projects/Ensemble"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nditah/backend-es6.git"
  },
  "keywords": [
    "Express",
    "RestAPI",
    "MongoDB",
    "ES6"
  ],
